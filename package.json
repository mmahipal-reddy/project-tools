{
  "name": "project-setup-app",
  "version": "3.8.0",
  "description": "Project Setup Application with Salesforce Integration",
  "main": "server/index.js",
  "scripts": {
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon server/index.js",
    "server:with-frontend": "./start-backend-with-frontend.sh",
